    do
	for DIR in $DIR1 $DIR1/include $DIR1/lib $DIR1/dlllib $DIR1/bin
	do
	    if test -r $DIR/blt.h; then
		BLT_HEADER_DIR=$DIR
	    fi
	    if test -r $DIR/${LIB_PREFIX}BLT$BLT_USE_VERSION$STLIB_SUFFIX; then
		BLT_LIB_DIR=$DIR
	    fi
	    if test -r $DIR/hd${HDF_999_VERSION}m.dll; then
		HDF_DLL_DIR=$DIR
