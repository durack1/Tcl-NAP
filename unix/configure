  ac_dummy="${exec_prefix}/bin:${PATH}:${TCL_SRC_DIR}/${PLATFORM_DIR}"
  for ac_dir in $ac_dummy; do 
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_path_WISH_PROG="$ac_dir/$ac_word"
      break
    fi
  done
  IFS="$ac_save_ifs"
  ;;
esac
fi
WISH_PROG="$ac_cv_path_WISH_PROG"
if test -n "$WISH_PROG"; then
  echo "$ac_t""$WISH_PROG" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

test -n "$WISH_PROG" && break
done
