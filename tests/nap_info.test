# $Id: nap_info.test,v 1.2 2001/11/01 00:25:41 dav480 Exp $

Test nap_info-1.1 {nap_info bytes} {
    [nap "{[nap_info bytes]} > 0"]
} {1 1}

Test nap_info-1.2 {nap_info sequence} {
    set s [[nap "0"] sequence]
    expr "$s == [nap_info sequence]"
} {1}

unset s

return
